pmid = {22039361},
title = {{Accelerated Profile HMM Searches.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3197634{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
