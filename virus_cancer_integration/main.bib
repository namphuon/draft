file = {:Users/namphuon/Documents/Mendeley Desktop/Forster et al. - 2015 - Vy-PER eliminating false positive detection of virus integration events in next generation sequencing data.pdf:pdf},
isbn = {2045-2322 (Electronic)
2045-2322 (Linking)},
issn = {2045-2322},
journal = {Scientific reports},
mendeley-groups = {Virus,Virus/Integration},
number = {November 2014},
pages = {11534},
pmid = {26166306},
title = {{Vy-PER: eliminating false positive detection of virus integration events in next generation sequencing data.}},
